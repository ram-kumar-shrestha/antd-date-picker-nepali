{
  "name": "antd-date-picker-nepali",
  "description": "This is a antd component for nepali datepicker.",
  "private": false,
  "keywords": [
    "antd",
    "date-picker",
    "react",
    "nepali"
  ],
  "author": {
    "name": "Ram Kumar Shrestha"
  },
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": "https://github.com/ram-kumar-shrestha/antd-date-picker-nepali/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ram-kumar-shrestha/antd-date-picker-nepali",
    "directory": "packages/react"
  },
  "scripts": {
